<div class='grid-template-rows: repeat(1, minmax(0, 1fr))'>
  {{#if events.length}}
    <div class='grid-rows-{1}'>
      <h3>Saved Events:</h3>

      {{#each events as |event|}}
        <div class='grid-rows-{1}'>
          <div class='grid-rows-{1}'>
            <h4>{{event.name}}</h4>
            {{{event.description}}}
          </div>
        </div>
      {{/each}}
    </div>
  {{/if}}
</div>

<button>Events</button>

<script src='./js/login.js'></script>